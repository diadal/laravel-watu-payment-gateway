{
  "name": "diadal/laravel-watu-payment-gateway",
  "description": "Fast Payment Gate-Way Nigeria",
  "keywords": [
    "payment",
    "master card",
    "visa card"
  ],
  "homepage": "https://github.com/diadal/laravel-watu-payment-gateway",
  "license": "MIT",
  "authors": [
    {
      "name": "Diadal Nigeria Ltd",
      "email": "info@diadal.com.ng",
      "homepage": "https://diadal.com.ng",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1",
    "ext-exif": "*",
    "ext-fileinfo": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Diadal\\Watu\\": "src"
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "test": "vendor/bin/phpunit"
  },
  "autoload-dev": {
    "psr-4": {
      "Diadal\\Watu\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Diadal\\Watu\\WatuServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
